{{#> layout}}
  <div class="row">
    {{#if verified }}
    <p>
      Key for <a href="/pks/lookup?op=get&search={{ userid }}"<span class="email">{{ userid }}</span></a> published.
    </p>
    {{else}}
    <p>
      Verification failed! You can <a href="/upload">try uploading again</a>.
    </p>
    {{/if}}
  </div>
{{/layout}}

